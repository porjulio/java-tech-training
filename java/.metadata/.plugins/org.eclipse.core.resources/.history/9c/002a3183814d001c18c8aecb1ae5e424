//                }
//            }
//            System.out.println("\n" +cities[i]);
//        }
		//reverse alphabetical
		for (int i = 0; i < 5; i++) 
        {
            for (int j = i + 1; j < 5; j++) { 
                if (cities[i].compareToIgnoreCase(cities[j])>0) 
                {
//                    temp = cities[i];
//                    cities[i] = cities[j];
//                    cities[j] = temp;
                	temp = cities[j];
                	cities[j] = cities[i];
                	cities[i] = temp;
                }
            }
            System.out.println("\n" +cities[i]);
        }
		//}while(choice=='y');
